<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expense_management">

   <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/background"
        android:label="@string/app_name"
        android:roundIcon="@drawable/background"
        android:supportsRtl="true"
        android:theme="@style/Theme.Expence_management">
        <activity android:name="com.example.expense_management.editHandler"></activity>
        <activity android:name="com.example.expense_management.detailed_data" />
        <activity android:name="com.example.expense_management.AddingToDatabase" />
        <activity android:name="com.example.expense_management.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>